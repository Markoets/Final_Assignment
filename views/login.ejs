<body>
    <h1>Sign in</h1>


    <a href="/register">Register</a>

    <section id="contact">
        <div class="container">
          <h2 class="text-uppercase text-center text-secondary mb-0">Login</h2>
          <div class="row">
            <div class="col-lg-8 mx-auto">
                <form action="/login" method="POST">
                <div class="control-group">
                  <div class="mb-0 form-floating controls pb-2"><input type="email" name="username" placeholder="enter your email"><small
                      class="form-text text-danger help-block"></small></div>
                </div>
                <div class="control-group">
                  <div class="mb-0 form-floating controls pb-2"><input type="password" name="password" placeholder ="enter your password"><small class="form-text text-danger help-block"></small></div>
                </div>

                <div id="success"></div>
                <div><button class="btn btn-primary btn-xl" id="sendMessageButton" type="submit">Send</button> 
                </div>

                
</form>

